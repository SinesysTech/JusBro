{
  "name": "@browserless.io/browserless",
  "version": "2.37.1",
  "license": "SSPL",
  "description": "The browserless platform",
  "author": "browserless.io",
  "type": "module",
  "types": "./src/exports.ts",
  "main": "./build/exports.js",
  "exports": {
    ".": "./build/exports.js"
  },
  "bin": {
    "browserless": "./bin/browserless.js"
  },
  "scripts": {
    "dev": "next dev",
    "clean:next": "node scripts/clean-next-lock.js",
    "build": "next build && npm run server:build",
    "start": "next start",
    "server:build": "cd server && tsc",
    "server:dev": "npm run server:build && env-cmd -f .env node server/build",
    "server:start": "env-cmd -f .env node server/build",
    "build:old": "npm run clean && npm run build:ts && npm run install:adblock && npm run build:schemas && npm run build:devtools && npm run build:openapi",
    "build:dev": "npm run build:old && npm run build:function && npm run install:debugger",
    "build:devtools": "node server/scripts/install-devtools.js",
    "build:function": "node server/scripts/build-function.js",
    "build:schemas": "node server/scripts/build-schemas.js",
    "build:openapi": "node server/scripts/build-open-api.js",
    "build:ts": "cd server && tsc",
    "build:tests": "npm run build:ts && npm run build:function && npm run install:debugger",
    "clean": "node server/scripts/clean.js",
    "install:adblock": "node server/scripts/install-adblock.js",
    "install:debugger": "node server/scripts/install-debugger.js",
    "install:browsers": "node node_modules/playwright-core/cli.js install chromium firefox webkit msedge",
    "install:dev": "npm run install:browsers && npm run install:debugger",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint . --ext .ts,.tsx --fix",
    "prepack": "npm run build:dev",
    "prettier": "prettier '{server,lib,components,app,.github}/**/*.{js,ts,tsx,json,yml,yaml,md}' --log-level error --write",
    "test": "npm run build:tests && cross-env DEBUG=quiet mocha",
    "coverage": "npm run build:ts && c8 --reporter=text --reporter=html --reporter=lcov cross-env DEBUG=quiet mocha",
    "pje:login": "node server/scripts/pje/login.js",
    "pje:scrape": "node server/scripts/pje/raspar-todos-processos.js",
    "test:multi-trt": "node --import ./scripts/ts-loader.mjs scripts/test-all-trts-scraping.ts",
    "test:trt-validation": "node --import ./scripts/ts-loader.mjs scripts/test-multi-trt.ts",
    "test:failed-trts": "npx tsx scripts/test-failed-trts.ts",
    "test:accessibility": "npx tsx scripts/test-accessibility.ts",
    "test:responsiveness": "npx tsx scripts/test-responsiveness.ts",
    "test:ux": "npm run test:accessibility && npm run test:responsiveness",
    "postinstall": "patch-package"
  },
  "files": [
    "assets/*",
    "bin/*",
    "build/*",
    "docker/*",
    "extensions/*",
    "external/*",
    "fonts/*",
    "scripts/*",
    "src/*",
    "static/*",
    "CHANGELOG.md",
    "tsconfig.json"
  ],
  "dependencies": {
    "@hapi/bourne": "^3.0.0",
    "@hapi/hoek": "^11.0.4",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^13.1.0",
    "date-fns": "^4.1.0",
    "debug": "^4.4.1",
    "del": "^8.0.1",
    "dotenv": "^17.2.3",
    "file-type": "^21.0.0",
    "get-port": "^7.1.0",
    "gradient-string": "^3.0.0",
    "http-proxy": "^1.18.1",
    "joi": "^18.0.1",
    "lighthouse": "^13.0.1",
    "lucide-react": "^0.548.0",
    "micromatch": "^4.0.8",
    "motion": "^12.23.24",
    "next": "^16.0.0",
    "pg": "^8.16.3",
    "playwright-1.51": "npm:playwright-core@1.51.1",
    "playwright-1.52": "npm:playwright-core@1.52.0",
    "playwright-1.53": "npm:playwright-core@1.53.2",
    "playwright-1.54": "npm:playwright-core@1.54.2",
    "playwright-core": "^1.56.1",
    "puppeteer": "^24.26.1",
    "puppeteer-core": "24.26.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "queue": "^7.0.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "sonner": "^2.0.7",
    "systeminformation": "^5.27.11",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "tar-fs": "^3.1.1",
    "vaul": "^1.1.2",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "optionalDependencies": {
    "@types/chai": "^5.2.3",
    "@types/debug": "^4.1.12",
    "@types/gradient-string": "^1.1.6",
    "@types/http-proxy": "^1.17.17",
    "@types/micromatch": "^4.0.10",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.9.1",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "assert": "^2.0.0",
    "c8": "^10.1.3",
    "chai": "^6.2.0",
    "cross-env": "^10.1.0",
    "env-cmd": "^11.0.0",
    "esbuild": "^0.25.11",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "eslint": "^9.38.0",
    "extract-zip": "^2.0.1",
    "gunzip-maybe": "^1.4.2",
    "marked": "^16.4.1",
    "mocha": "^11.7.4",
    "move-file": "^3.1.0",
    "prettier": "^3.6.2",
    "sinon": "^21.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-json-schema": "^0.65.1"
  },
  "playwrightVersions": {
    "default": "playwright-core",
    "1.55": "playwright-core",
    "1.54": "playwright-1.54",
    "1.53": "playwright-1.53",
    "1.52": "playwright-1.52",
    "1.51": "playwright-1.51"
  },
  "engines": {
    "node": ">= 24 < 25"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "node_modules/*",
      "static/*",
      "**.spec.ts",
      "build/*",
      ".DS_Store",
      ".no-git/*",
      "*.log"
    ],
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "no-async-promise-executor": 0,
      "@typescript-eslint/ban-ts-comment": 0,
      "@typescript-eslint/no-var-requires": 0,
      "sort-imports": [
        "error",
        {
          "ignoreCase": false,
          "ignoreDeclarationSort": false,
          "ignoreMemberSort": false,
          "memberSyntaxSortOrder": [
            "none",
            "all",
            "multiple",
            "single"
          ],
          "allowSeparatedGroups": true
        }
      ]
    }
  },
  "mocha": {
    "extension": [
      "js"
    ],
    "loader": "ts-node/esm",
    "spec": "build/**/*.spec.js",
    "timeout": 45000,
    "slow": 5000
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 80
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.11",
    "happy-dom": "^20.0.8",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.6",
    "prisma": "^6.18.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "glob": "^11.0.0",
    "rimraf": "^6.0.0",
    "inflight": "^2.0.0"
  }
}
